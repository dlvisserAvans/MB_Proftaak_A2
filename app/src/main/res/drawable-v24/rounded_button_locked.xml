<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="@color/color_btn_pressed"/>
            <stroke
                android:width="2px"
                android:color="#FFFFFF" />
            <corners
                android:radius="8dp" />

        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/color_btn_locked"/>
            <stroke
                android:width="2px"
                android:color="@color/color_btn_unpressed_outline"/>
            <corners
                android:radius="8dp" />
        </shape>
    </item>
</selector>